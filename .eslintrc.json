{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "camelcase": "warn",
        "semi": "warn",
        "callback-return": "error",
        "no-global-assign": "off",
        "constructor-super": "error",
        "no-var": "error"
    }
}